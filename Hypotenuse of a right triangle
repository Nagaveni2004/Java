import java.util.Scanner;

public class Main {
    public static void main(String[] args)
    {
        Scanner scanner=new Scanner(System.in);
        double a;
        double b;
        double c;
        System.out.print("Enter the length of side A:");
        a=scanner.nextDouble();
        System.out.print("Enter the length of side B:");
        b=scanner.nextDouble();
        c=Math.sqrt(Math.pow(a,2)+Math.pow(b,2));
        System.out.print("The hypotenuse (side c)is:"+c);
    }
}

//OUTPUT
a)Enter the length of side A:3
  Enter the length of side B:4
  The hypotenuse (side c)is:5.0

b)Enter the length of side A:2
  Enter the length of side B:5
  The hypotenuse (side c)is:5.385164807134504

c)Enter the length of side A:5
  Enter the length of side B:18
  The hypotenuse (side c)is:18.681541692269406
